      '     e.NOME,'#13#10'       C.ITENS'#13#10'from CONTROLE C'#13#10'join ESTUFAS e on' +
      ' (e.CODESTUFAS = c.CODESTUFA)'#13#10'where c.CODCONTROLE = :CODCONTROL' +
